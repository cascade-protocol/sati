{
  "name": "@cascade-fyi/sati-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for SATI - Solana Agent Trust Infrastructure",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./light": {
      "import": {
        "types": "./dist/light/index.d.mts",
        "default": "./dist/light/index.mjs"
      },
      "require": {
        "types": "./dist/light/index.d.cts",
        "default": "./dist/light/index.cjs"
      }
    },
    "./web3-compat": {
      "import": {
        "types": "./dist/web3-compat/index.d.mts",
        "default": "./dist/web3-compat/index.mjs"
      },
      "require": {
        "types": "./dist/web3-compat/index.d.cts",
        "default": "./dist/web3-compat/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**",
    "README.md",
    "idl.json",
    "types.ts"
  ],
  "scripts": {
    "generate": "cp ../../target/idl/sati.json ./idl.json && tsx scripts/generate-clients.ts",
    "build": "pnpm generate && tsdown",
    "prepublishOnly": "pnpm build",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:watch": "vitest tests/unit tests/integration",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist src/generated"
  },
  "keywords": [
    "solana",
    "sati",
    "agent-infrastructure",
    "erc-8004",
    "ap2"
  ],
  "author": "tenequm",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@solana-program/token-2022": "^0.6.1",
    "@solana/kit": "^5.0.0",
    "@solana/web3.js": "^1.98.0"
  },
  "peerDependenciesMeta": {
    "@solana/web3.js": {
      "optional": true
    }
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@lightprotocol/stateless.js": "0.22.1-alpha.5",
    "@noble/hashes": "^1.7.2",
    "@solana-program/address-lookup-table": "^0.10.0",
    "@solana-program/compute-budget": "^0.11.0",
    "@solana-program/token-2022": "^0.6.1",
    "@solana/kit": "^5.0.0",
    "bs58": "^6.0.0",
    "sas-lib": "^1.0.10",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.3.4",
    "@codama/renderers-js": "^1.5.1",
    "@codama/visitors": "^1.4.2",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "codama": "^1.4.2",
    "litesvm": "^0.4.0",
    "ts-node": "^10.9.0",
    "tsdown": "^0.16.1",
    "tsx": "^4.20.6",
    "tweetnacl": "^1.0.3",
    "typescript": "^5.7.3",
    "vitest": "^4.0.15"
  }
}
