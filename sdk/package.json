{
  "name": "@sati/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for SATI - Solana Agent Trust Infrastructure",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**",
    "README.md",
    "idl.json",
    "types.ts"
  ],
  "scripts": {
    "generate": "cp ../target/idl/sati_registry.json ./idl.json && tsx scripts/generate-clients.ts",
    "build": "pnpm generate && tsdown",
    "prepublishOnly": "pnpm build",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist src/generated"
  },
  "keywords": [
    "solana",
    "sati",
    "agent-infrastructure",
    "erc-8004",
    "ap2"
  ],
  "author": "tenequm",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@solana-program/token-2022": "^0.6.1",
    "@solana/kit": "^5.0.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@noble/hashes": "^1.7.2",
    "@solana-program/token-2022": "^0.6.1",
    "@solana/kit": "^5.0.0",
    "bs58": "^6.0.0",
    "sas-lib": "^1.0.10"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.3.4",
    "@codama/renderers-js": "^1.5.1",
    "@codama/visitors": "^1.4.2",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@types/node": "^20.0.0",
    "codama": "^1.4.2",
    "litesvm": "^0.4.0",
    "ts-node": "^10.9.0",
    "tsdown": "^0.16.1",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "vitest": "^4.0.15"
  }
}
